          <value type="string" datatypeLibrary="http://whattf.org/datatype-draft"></value>
        </choice>
      </attribute>
      <ref name="v5only"/>
    </interleave>
  </define>
  <define name="script.attrs.type">
    <attribute>
      <name ns="">type</name>
      <choice>
        <value type="string" datatypeLibrary="http://whattf.org/datatype-draft"></value>
        <value type="string" datatypeLibrary="http://whattf.org/datatype-draft">module</value>
        <value type="string" datatypeLibrary="http://whattf.org/datatype-draft">importmap</value>
        <value type="string" datatypeLibrary="http://whattf.org/datatype-draft">speculationrules</value>

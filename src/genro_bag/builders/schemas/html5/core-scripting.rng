      <element>
        <name ns="">noscript</name>
        <interleave>
          <ref name="noscript.inner.head"/>
          <ref name="noscript.attrs"/>
        </interleave>
      </element>
      <ref name="HTMLonly"/>
    </interleave>
  </define>
  <define name="noscript.inner.head">
    <zeroOrMore>
      <choice>
        <ref name="link.elem"/>
        <ref name="meta.http-equiv.default-style.elem"/>
        <ref name="meta.http-equiv.refresh.elem"/>
        <ref name="style.elem"/>
      </choice>
    </zeroOrMore>
  </define>

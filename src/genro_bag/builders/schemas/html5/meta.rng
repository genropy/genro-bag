      <name ns="">http-equiv</name>
      <value type="string" datatypeLibrary="http://whattf.org/datatype-draft">default-style</value>
    </attribute>
  </define>
  <define name="meta.http-equiv.attrs.content.default-style">
    <attribute>
      <name ns="">content</name>
      <ref name="common.data.default-style"/>
